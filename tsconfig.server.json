  "compilerOptions": {
    "module": "ESNext",
    "moduleResolution": "node",
    "outDir": "dist",
    "lib": ["ES2022"],
    "target": "ES2022",
    "isolatedModules": false,
    "noEmit": false,
    "esModuleInterop": true
  },
  "include": ["server.ts", "lib/**/*.ts"]
}
